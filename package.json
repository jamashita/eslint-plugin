{
  "name": "@jamashita/eslint-plugin",
  "version": "1.93.2",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/eslint-plugin.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "main": "./index.js",
  "files": [
    "configs",
    "index.js"
  ],
  "scripts": {
    "commit": "git cz",
    "format": "eslint --fix 'examples/**/*.{ts,tsx}' --config configs/recommended.js",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --deep --upgrade"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "13.4.19",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-solid": "0.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.4",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.6",
    "@semantic-release/npm": "11.0.0",
    "@semantic-release/release-notes-generator": "11.0.7",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "git-cz": "4.9.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "14.0.1",
    "npm-check-updates": "16.14.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "22.0.0",
    "sort-package-json": "2.5.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18.x"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  }
}
