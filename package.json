{
  "name": "@jamashita/eslint-plugin",
  "version": "1.48.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/eslint-plugin.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "main": "./index.js",
  "scripts": {
    "commit": "git cz",
    "format": "eslint 'examples/**/*.{ts,tsx}'",
    "release": "semantic-release",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --upgrade",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "@typescript-eslint/typescript-estree": "4.29.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/node": "16.4.10",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "eslint": "7.32.0",
    "git-cz": "4.7.6",
    "husky": "^7.0.1",
    "is-ci": "3.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.1",
    "npm-check-updates": "11.8.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semantic-release": "17.4.4",
    "sort-package-json": "1.50.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "eslint": "7.32.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
