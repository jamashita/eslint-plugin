{
  "name": "@jamashita/eslint-plugin",
  "version": "1.76.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/eslint-plugin.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "main": "./index.js",
  "scripts": {
    "commit": "git cz",
    "format": "eslint --fix 'examples/**/*.{ts,tsx}' --config configs/recommended.js",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --deep --upgrade"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "git-cz": "4.9.0",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.4",
    "npm-check-updates": "16.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "19.0.5",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.3"
  },
  "peerDependencies": {
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
