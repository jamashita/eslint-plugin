{
  "name": "@jamashita/eslint-plugin",
  "version": "1.52.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/eslint-plugin.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "main": "./index.js",
  "scripts": {
    "commit": "git cz",
    "format": "eslint 'examples/**/*.{ts,tsx}'",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --upgrade"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@typescript-eslint/typescript-estree": "5.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "eslint": "8.2.0",
    "git-cz": "4.8.0",
    "husky": "^7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "npm-check-updates": "12.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semantic-release": "18.0.0",
    "sort-package-json": "1.52.0",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "eslint": "8.2.0",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
